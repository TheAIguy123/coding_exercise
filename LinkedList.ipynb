    "def rotate_list(head, k):\n",
    "    fast = slow = head \n",
    "    for _ in range(k):\n",
    "        fast = fast.next \n",
    "    if not fast: return head \n",
    "    while fast.next: \n",
    "        fast, slow = fast.next, slow.next \n",
    "    tmp, slow.next = slow.next, None \n",
    "    fast.next = head \n",
    "    return tmp \n",
