    "# build from list \n",
    "\n",
    "def build_linked_list(vals):\n",
    "    if not vals: return None \n",
    "    head = curr = Node(vals[0])\n",
    "    if len(vals) > 1: \n",
    "        for val in vals[1:]:\n",
    "            curr.next = Node(val) \n",
    "            curr = curr.next \n",
    "    return head \n",
    "\n",
    "def llist2str(head):\n",
    "    if not head: return 'empty linked list'\n",
    "    res = ''\n",
    "    curr = head \n",
    "    while curr: \n",
    "        res += str(curr.val) \n",
    "        if curr.next: \n",
    "            res += '->'\n",
    "        curr = curr.next\n",
    "    return res \n",
    "    "
   ]
