    "# using 3 pointers \n",
    "def isPanlindrome(head):\n",
    "    fast = slow = head \n",
    "    prev = None \n",
    "    while fast and fast.next: \n",
    "        slow, fast, prev, prev.next = slow.next, fast.next.next, slow, prev \n",
    "    \n",
    "    if fast: # odd elems\n",
